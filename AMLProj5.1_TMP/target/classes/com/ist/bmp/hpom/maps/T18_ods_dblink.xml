  </statement> 
 
  <statement id="lockT18_ods2dblink" parameterClass="java.lang.String" resultClass="com.ist.bmp.hpom.dto.T18_ods_dblink"> 
  	update T18_ods_dblink set islock ='1' where tableename=#value# 
  </statement> 
  
