<?xml version="1.0" encoding="UTF-8"?> 
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd"> 
 
<sqlMap namespace="T09_indic_intou"> 
  <statement id="insertT09_indic_inout" parameterClass="com.ist.iap.dto.T09_indic_inout" resultClass="int"> 
  insert into T09_indic_inout(
  indickey,tabkey,tabename,des
  ) values(
  #indickey#,#tabkey#,#tabename#,#des#
  ) 
  </statement> 
 
  <statement id="deleteT09_indic_inout" parameterClass="java.lang.String" resultClass="int">
  delete from T09_indic_inout where indickey=#value# 
  </statement> 
 
  <statement id="getT09_indic_inoutDisp" parameterClass="java.lang.String" resultClass="com.ist.iap.dto.T09_indic_inout"> 
  select * from T09_indic_inout where indickey=#value# 
  </statement> 
 
</sqlMap> 

